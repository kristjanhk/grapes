{
  "compilerOptions": {
    "outDir": "./gateway/main/resources/static/dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "music/gen/*": ["music/main/gen/*"],
      "mikrotik/gen/*": ["mikrotik/main/gen/*"],
      "music/*": ["music/ui/*"],
      "mikrotik/*": ["mikrotik/ui/*"]
    }
  }
}